<%= form_tag(search_our_locations_path, method: :get) do %>
  <%= text_field_tag :search, '', id: 'rockport-lookup', placeholder: "Enter Address, City, or Zip Code" %>
  <%= submit_tag 'Search Locations', class: 'styled-link selected nearest-locations', 'data-text-locations': "Search Locations"%>
<% end %>
<button class="styled-link current-locations modal-locations" onclick="searchWithCurrentLocation('location')">Search by Current Location</button>
